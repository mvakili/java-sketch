package array;

struct Array_bit {
  int length;
  bit[length] A;
}

struct Array_char {
  int length;
  char[length] A;
}

struct Array_int {
  int length;
  int[length] A;
}

struct Array_float {
  int length;
  float[length] A;
}

struct Array_double {
  int length;
  double[length] A;
}

struct Array_Object {
  int length;
  Object[length] A;
}

Object Wrap_Array_bit(Array_bit arr) {
  return new Object(__cid=Array(), _array_bit=arr); 
}

Object Wrap_Array_char(Array_char arr) {
  return new Object(__cid=Array(), _array_char=arr); 
}

Object Wrap_Array_int(Array_int arr) {
  return new Object(__cid=Array(), _array_int=arr); 
}

Object Wrap_Array_float(Array_float arr) {
  return new Object(__cid=Array(), _array_float=arr); 
}

Object Wrap_Array_double(Array_double arr) {
  return new Object(__cid=Array(), _array_double=arr); 
}

Object Wrap_Array_Object(Array_Object arr) {
  return new Object(__cid=Array(), _array_object=arr); 
}

